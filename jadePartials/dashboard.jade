ion-header-bar.bar-positive(align-title='center')
  .title
    span.fa.fa-binoculars.fa-rotate-90.fa-spin
    strong
    | &nbsp;Dr. Track
ion-content.center
  ion-list
    ion-item.item-icon-left(ng-repeat='dashboardOption in dashboardOptions track by $index', href='#/{{dashboardOption.name | lowercase}}') 
      i(class="{{dashboardOption.icon}}")
      |{{dashboardOption.name}}
ion-footer-bar.bar-positive(align-title='center')
  .title Dashboard
